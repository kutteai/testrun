# PayCio Wallet Environment Configuration
# Copy this file to .env and replace with your actual API keys

# Required API Keys
# You MUST set these for the wallet to function properly

# Infura Project ID (REQUIRED)
# Get it from: https://infura.io
INFURA_PROJECT_ID=your_infura_project_id_here

# Recommended API Keys
# These are highly recommended for full functionality

# Etherscan API Key (RECOMMENDED)
# Get it from: https://etherscan.io
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# CoinGecko API Key (RECOMMENDED)
# Get it from: https://www.coingecko.com/en/api
COINGECKO_API_KEY=your_coingecko_api_key_here

# OpenSea API Key (RECOMMENDED)
# Get it from: https://docs.opensea.io/reference/api-overview
OPENSEA_API_KEY=your_opensea_api_key_here

# Optional API Keys
# These provide additional features but are not required

# Alchemy API Key (OPTIONAL)
# Get it from: https://www.alchemy.com
ALCHEMY_API_KEY=your_alchemy_api_key_here

# BSCScan API Key (OPTIONAL)
# Get it from: https://bscscan.com
BSCSCAN_API_KEY=your_bscscan_api_key_here

# PolygonScan API Key (OPTIONAL)
# Get it from: https://polygonscan.com
POLYGONSCAN_API_KEY=your_polygonscan_api_key_here

# AvalancheScan API Key (OPTIONAL)
# Get it from: https://snowtrace.io
AVALANCHESCAN_API_KEY=your_avalanchescan_api_key_here

# ArbitrumScan API Key (OPTIONAL)
# Get it from: https://arbiscan.io
ARBITRUMSCAN_API_KEY=your_arbitrumscan_api_key_here

# OptimismScan API Key (OPTIONAL)
# Get it from: https://optimistic.etherscan.io
OPTIMISMSCAN_API_KEY=your_optimismscan_api_key_here

# CoinMarketCap API Key (OPTIONAL)
# Get it from: https://coinmarketcap.com/api
COINMARKETCAP_API_KEY=your_coinmarketcap_api_key_here

# Alchemy NFT API Key (OPTIONAL)
# Use the same key as ALCHEMY_API_KEY
ALCHEMY_NFT_API_KEY=your_alchemy_api_key_here

# DeFi Pulse API Key (OPTIONAL)
# Get it from: https://defipulse.com
DEFI_PULSE_API_KEY=your_defi_pulse_api_key_here

# Optional - Custom ENS RPC (if not using Infura)
# Leave this empty to use Infura for ENS resolution
ENS_RPC_URL=https://mainnet.infura.io/v3/your_infura_project_id_here

# Environment
NODE_ENV=development 